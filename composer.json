{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "convergine/craft-content-buddy": "1.1.8",
    "craftcms/ckeditor": "4.2.0",
    "craftcms/cms": "5.4.6",
    "craftcms/element-api": "4.1.0",
    "craftcms/feed-me": "6.4.0",
    "craftcms/postmark": "3.1.0",
    "nystudio107/craft-code-field": "5.0.0",
    "nystudio107/craft-minify": "5.0.0",
    "nystudio107/craft-seomatic": "5.1.4",
    "shornuk/craft-plausible": "3.0.0",
    "symfony/css-selector": "^7.1",
    "symfony/dom-crawler": "^7.1",
    "topshelfcraft/wordsmith": "5.0.0",
    "verbb/navigation": "3.0.4",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.1",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\fetchprofile\\": "modules/fetch-profile/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}